{
    "name": "Consulta Insta",
    "version": "1.0",
    "description": "Consulta o instagram pelo @",
    "browser_action": {
        "default_popup":"index.html",
        "default_icon": "icon.png"
    },
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "activeTab",
        "contextMenus"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "background":{
        "scripts": [ "background.js" ] 
    }
}
